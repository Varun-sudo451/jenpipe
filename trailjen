pipeline {
	tools{
		jdk 'masterjavahome'
		maven 'mastermvnhome'
	}
	
	agent none
	stages{
		stage('Checkout'){
			agent any
			steps{
				git 'https://github.com/ThoufiqB/game-of-life.git'
			}
		}
		stage('compile'){
			agent any
			steps{
			 	sh 'mvn compile'
			}
		}
		stage('Test'){
			agent any
			steps{
			 	sh 'mvn test'
			}
		}
		stage('Package'){
			agent any
			steps{
			 	sh 'mvn package'
			}
		}
	}
}
